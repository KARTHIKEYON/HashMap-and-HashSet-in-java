import java.util.*;
class Main{
    public static void main(String args[]){
        String str1="programming";
        HashSet<Character>set=new HashSet<>();
        for(char c: str1.toCharArray()){
            if(!set.add(c)){
                System.out.println("repeated character is : "+c);
                break;
            }
        }
    }
}

output:

repeated character is : r
